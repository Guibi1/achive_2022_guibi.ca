{
  "name": "guibi.ca",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "autoprefixer": "^10.3.5",
    "file-saver": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.0.0",
    "postcss-nested": "^5.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-swipeable-views": "^0.14.0",
    "react-youtube": "^7.13.1",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "build:css": "postcss ./src/styles/index.css -o ./src/processedStyles/index.css",
    "build:cssHeader": "postcss ./src/styles/Header.css -o ./src/processedStyles/Header.css",
    "watch:css": "postcss ./src/styles/index.css -o ./src/processedStyles/index.css -w",
    "watch:cssHeader": "postcss ./src/styles/Header.css -o ./src/processedStyles/Header.css -w",
    "start": "npm-run-all -p watch:css start-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build:css build:cssHeader build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
